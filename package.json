    "@slidev/client": ">=52.0.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@slidev/cli": "^52.0.0",
    "@slidev/theme-default": "latest",
    "@types/uuid": "^10.0.0",
